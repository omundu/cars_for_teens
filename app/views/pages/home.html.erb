<h1>Pages#home</h1>
<p>Find me in app/views/pages/home.html.erb</p>

<p>
  <ul>
    <% @cars.cars.each do |car| %>
     <!-- <li> <%= car.join(" :: ") %> </li> -->
     <% vehicle, year, price = car.first.split, car[1], car.last %>
     <% manufacturer, *make = vehicle %>
     <li> <%= manufacturer %>
       <ul>
         <li><%= make.join(" ") %></li>
         <li><%= year %></li>
         <li><%= price %></li>
       </ul>
     </li>
    <% end %>
  </ul>
</p>


